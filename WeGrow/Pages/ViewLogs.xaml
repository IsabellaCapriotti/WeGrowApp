<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WeGrow.Pages.ViewLogs"
             NavigationPage.HasBackButton="True">
    <ContentPage.Content>
        <ScrollView BackgroundColor="Wheat" Orientation="Vertical" VerticalOptions="FillAndExpand">
            <StackLayout Orientation="Vertical" x:Name="mainLayout" Spacing="30" Padding="20">
                
               <Button x:Name="newLogButton" Text="Log Today" Clicked="newLogButton_Clicked" BackgroundColor="#2c610e" TextColor="White"/>
               
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>